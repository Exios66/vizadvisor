    "goal": {
      "description": "Show monthly revenue trend over the year",
      "category": "trend",
      "primaryQuestion": "How has revenue changed over time?"
    },
    "parameters": {
      "audience": "Business stakeholders",
      "library": "recharts",
      "language": "javascript",
      "interactivity": "Tooltips only",
      "accessibility": "WCAG AA",
      "extraNotes": null
    }
  },
  "expected": {
    "goal_category": "trend",
    "primary_chart_type_contains": "line",
    "must_reference_column": "revenue"
  }
