{
  "description": "Sales trend over time",
  "input": {
    "sessionId": "test-trend-001",
    "dataset": {
      "rowCount": 365,
      "schema": [
        { "name": "date", "type": "temporal", "cardinality": 365, "nullable": false, "range": "2024-01-01 → 2024-12-31" },
        { "name": "revenue", "type": "quantitative", "cardinality": 365, "nullable": false, "range": "1,000 – 50,000" }
